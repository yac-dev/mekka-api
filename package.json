{
  "name": "server",
  "version": "1.0.1",
  "description": "Mekka api project",
  "author": "Yosuke Kojima ",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node --es-module-specifier-resolution=node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server:dev": "env-cmd -f ./config/.env.dev nodemon --es-module-specifier-resolution=node src/index.js",
    "server:staging-heroku": "NODE_ENV=staging node --es-module-specifier-resolution=node src/index.js",
    "server:staging": "env-cmd -f .env node --es-module-specifier-resolution=node src/index.js",
    "server:staging-ec2": "env-cmd -f ./.env node --es-module-specifier-resolution=node src/index.js",
    "server:production": "NODE_ENV=production node --es-module-specifier-resolution=node src/index.js",
    "vercel-build": "echo hello vercel",
    "console": "node run console",
    "seed:clear": "env-cmd -f ./config/.env.dev node src/seeders/index.js clear",
    "seed:dev": "env-cmd -f ./config/.env.dev node src/seeders/index.js seed",
    "seed:reset": "env-cmd -f ./config/.env.dev node src/seeders/index.js reset",
    "migrate-create:dev": "env-cmd -f ./config/.env.dev npx migrate-mongo create add-new-field",
    "migrate-up:dev": "env-cmd -f ./config/.env.dev npx migrate-mongo up",
    "migrate-down:dev": "env-cmd -f ./config/.env.dev npx migrate-mongo down"
  },
  "type": "module",
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/lib-storage": "^3.456.0",
    "@imgly/background-removal-node": "^1.4.5",
    "@sendgrid/mail": "^8.1.1",
    "aws-sdk": "^2.1414.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.1",
    "mailgun.js": "^10.2.1",
    "migrate-mongo": "^11.0.0",
    "mongoose": "^7.3.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.11",
    "sharp": "^0.32.6",
    "uuid": "^9.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
